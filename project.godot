config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false
resources_spreadsheet_view/fold_docks=true
resources_spreadsheet_view/resource_cell_label_mode=1
resources_spreadsheet_view/freeze_first_column=true

[application]

config/name="Pokemon TCG"
config/tags=PackedStringArray("2d", "made", "test")
run/main_scene="uid://dnij7qm62bibx"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://Art/Energy/48px-Metal-attack.png"

[autoload]

Constants="*res://Code/Globals/Constants.gd"
Globals="*res://Code/Globals/globals.gd"
Database="*res://Code/Globals/database.gd"
SignalBus="*res://Code/Globals/signal_bus.gd"
Conversions="*res://Code/Globals/conversions.gd"

[debug]

gdscript/warnings/unused_parameter=0

[display]

window/size/viewport_width=1080
window/size/viewport_height=720

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/resources_spreadsheet_view/plugin.cfg")

[file_customization]

folder_colors={
"res://Art/": "red",
